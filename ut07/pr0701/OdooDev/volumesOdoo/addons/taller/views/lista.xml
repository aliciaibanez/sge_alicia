<odoo>
    <template id="lista">
        <t t-call="web.html_container">
            <div class="container">
                <h1>Listado de Veh√≠culos</h1>

                <t t-foreach="vehiculos" t-as="vehiculo">
                    <div class="vehiculo">
                        <span>
                            <t t-esc="vehiculo.matricula"/>
                        </span>
                        <span>
                            <t t-esc="vehiculo.marca"/>
                        </span>
                        <span>
                            <t t-esc="vehiculo.modelo"/>
                        </span>
                        <span>
                            <t t-esc="vehiculo.anio"/>
                        </span>
                    </div>
                </t>

            </div>
        </t>
    </template>
</odoo>
